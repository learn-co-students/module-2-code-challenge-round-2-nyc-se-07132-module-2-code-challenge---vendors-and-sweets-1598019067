<h1>VendorSweet New Form</h1>

<%if flash[:my_error]%>
    <%flash[:my_error].each do |error|%>
    <%=error%>
    <%end%>
<%end%>

<%= form_for @vendor_sweet do |f| %>
    <%=f.collection_select :vendor_id, @vendor, :id, :name %>
    <%=f.collection_select :sweet_id, @sweet, :id, :name %>
    <%=f.number_field :price%>
    <%=f.submit%>
<% end %>