<% if flash[:myerror] %>
<% flash[:myerror].each do |error| %>
<%= error %><br>
<% end %>
<% end %>
<br>
<br>




<%= form_for(@vendor_sweet) do |f| %>
    <%= f.label :price %>
    <%= f.number_field :price %><br><br>

    <%= f.label :vendor %>
    <%= f.collection_select :vendor_id, Vendor.all, :id, :name %><br><br>

    <%= f.label :sweet %>
    <%= f.collection_select :sweet_id, Sweet.all, :id, :name %>

    <%= f.submit "Create!" %>
<% end %>